{
  "name": "token-swap-dapp",
  "version": "0.1.0",
  "description": "Dapp and example Solidity contracts demonstrating a straight-forward token swap.",
  "homepage": "http://shopinswap.netlify.com",
  "repository": "github:auser/token-swap-dapp",
  "bugs": "http://github.com/auser/token-swap-dapp/issues",
  "author": "Ari Lerner <ari@shishi.to>",
  "license": "MIT",
  "keywords": [
    "solidity",
    "dapp",
    "contracts",
    "token",
    "tokens",
    "token swap",
    "truffle"
  ],
  "dependencies": {
    "autoprefixer": "6.5.1",
    "babel-core": "6.17.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "17.0.2",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^2.0.1",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^2.0.2",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.0",
    "detect-port": "1.0.1",
    "dotenv": "^2.0.0",
    "eslint": "3.8.1",
    "eslint-config-react-app": "^0.5.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "ethereumjs-abi": "^0.6.5",
    "executive": "^1.6.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "fs-extra": "0.30.0",
    "ganache-cli": "^6.1.4",
    "gzip-size": "3.0.0",
    "hanzo-solidity": "^0.1.5",
    "html-webpack-plugin": "2.24.0",
    "html2canvas": "^1.0.0-alpha.12",
    "http-proxy-middleware": "0.17.2",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "jspdf": "^1.4.1",
    "object-assign": "4.1.0",
    "path-exists": "2.1.0",
    "postcss-loader": "1.0.0",
    "promise": "7.1.1",
    "react": "^15.4.2",
    "react-dev-utils": "^0.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "recursive-readdir": "2.1.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.11",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "thunk": "0.0.1",
    "truffle": "^4.1.13",
    "truffle-contract": "^1.1.8",
    "truffle-hdwallet-provider": "0.0.5",
    "truffle-solidity-loader": "0.0.8",
    "truffle-wallet-provider": "0.0.5",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "1.0.0",
    "winston": "^3.0.0",
    "zeppelin-solidity": "^1.10.0"
  },
  "scripts": {
    "build": "truffle compile && node scripts/build.js",
    "dev": "node scripts/dev.js",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
